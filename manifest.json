{
  "manifest_version": 3,
  "name": "Lichess Latency Checker",
  "version": "1.0",
  "description": "Measures and displays latency to Lichess.org using HTTP requests.",
  "host_permissions": [
      "https://lichess.org/*"
  ],
  "content_scripts": [
      {
          "matches": ["https://lichess.org/*"],
          "js": ["content.js"],
          "css": ["styles.css"]
      }
  ]
}
